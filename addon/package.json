{
  "name": "ember-page-title",
  "version": "8.0.0-beta.0",
  "description": "Page Titles for Ember applications",
  "keywords": [
    "ember-addon",
    "page title",
    "title",
    "component",
    "fastboot"
  ],
  "license": "MIT",
  "author": "Tim Evans <tim.c.evans@me.com>",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./test-support": "./dist/test-support/index.js",
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "addon-main.cjs",
    "dist"
  ],
  "repository": "https://github.com/ember-cli/ember-page-title",
  "scripts": {
    "build": "rollup --config --bundleConfigAsCjs",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "start": "rollup --config --bundleConfigAsCjs --watch",
    "prepublishOnly": "rollup --config --bundleConfigAsCjs"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.22.7",
    "@babel/plugin-transform-typescript": "^7.22.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-typescript": "^7.22.5",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@embroider/addon-dev": "^3.2.0",
    "@simple-dom/document": "^1.4.0",
    "@tsconfig/ember": "^3.0.0",
    "@types/ember-cli-fastboot": "^2.2.2",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "concurrently": "^8.2.0",
    "ember-source": "^5.1.2",
    "ember-template-lint": "^5.11.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^11.9.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "rollup": "^3.26.3",
    "rollup-plugin-ts": "^3.2.0",
    "typescript": "~5.1.6"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember-addon": {
    "demoURL": "https://ember-cli.github.io/ember-page-title",
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2,
    "app-js": {
      "./helpers/page-title.js": "./dist/_app_/helpers/page-title.js",
      "./services/page-title.js": "./dist/_app_/services/page-title.js"
    }
  },
  "homepage": "https://ember-cli.github.io/ember-page-title",
  "volta": {
    "extends": "../package.json"
  }
}
