{
  "name": "docs",
  "version": "8.1.0",
  "description": "Page Titles for Ember applications",
  "keywords": [
    "page title",
    "title",
    "component",
    "fastboot"
  ],
  "license": "MIT",
  "author": "Tim Evans <tim.c.evans@me.com>",
  "repository": "https://github.com/ember-cli/ember-page-title",
  "scripts": {
    "build": "ember build --environment=production",
    "format:hbs": "prettier **/*.hbs --write --parser=glimmer",
    "format:js": "prettier **/*.js --write",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "release": "standard-version",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.1",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.7.2",
    "ember-cli": "~4.12.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-code-coverage": "^2.0.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-update": "^2.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-inline-svg": "^1.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "8.1.0",
    "ember-qunit": "^8.0.2",
    "ember-resolver": "^11.0.1",
    "ember-source": "~3.28.3",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.13.0",
    "ember-truth-helpers": "^4.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^11.12.0",
    "eslint-plugin-jsonc": "^2.11.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-qunit": "^8.0.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "qunit": "^2.16.0",
    "sass": "^1.43.3",
    "standard-version": "^9.3.2",
    "webpack": "^5.0.0"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "homepage": "https://ember-cli.github.io/ember-page-title",
  "volta": {
    "extends": "../package.json"
  }
}